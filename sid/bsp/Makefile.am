## Process this with automake to create Makefile.in

AUTOMAKE_OPTIONS = foreign
ACLOCAL_AMFLAGS = -I ../config

SUBDIRS = . pregen

if SIDTARGET_ARM
FAM0SCRIPTS = arm-elf-sid
else

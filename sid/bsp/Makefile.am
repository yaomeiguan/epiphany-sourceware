endif

if SIDTARGET_M68K
FAM3SCRIPTS = m68k-elf-sid
else
FAM3SCRIPTS =
endif

FAM4SCRIPTS =

FAM5SCRIPTS =

FAM6SCRIPTS =

FAM7SCRIPTS =

ALLFAMSCRIPTS = $(FAM0SCRIPTS) $(FAM1SCRIPTS) $(FAM2SCRIPTS) $(FAM3SCRIPTS) \
		$(FAM4SCRIPTS) $(FAM5SCRIPTS) $(FAM6SCRIPTS) $(FAM7SCRIPTS)

bin_SCRIPTS = configrun-sid $(ALLFAMSCRIPTS)

# mep testcase for sw $rn3,$disp7a4($tp) -*- Asm -*-
# mach: all

	.include "testutils.inc"
	start
	.global sw_tp
sw_tp:
	# Store 0x3511 at 0x3c.
	mov $tp, 0x30
	mov $3, 0x35
	sll $3, 8
	add $3, 0x11
	sw $3, 12($tp)

	# Load it back.
	mov $0, 0x3c
	lw $1, ($0)

	# Compare.
	sub $1, $3
	bnez $1, wrong
okay:
	pass
wrong:
	fail

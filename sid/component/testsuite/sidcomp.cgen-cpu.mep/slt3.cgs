# mep testcase for slt3 $0,$rn,$rm -*- Asm -*-
# mach: all

	.include "testutils.inc"
	start
	.global slt3
slt3:
	mov $1, 1
	mov $2, 3
	# 1 < 3 is true ($0 <= 1).
	slt3 $0, $1, $2
	assert $0, 1

	# 3 < 1 is false ($0 <= 0).
	slt3 $0, $2, $1
	assert $0, 0

	mov $1, -1
	mov $2, 3
	# -1 < 3 is true ($0 <= 1).
	slt3 $0, $1, $2
	assert $0, 1

	# 3 < -1 is false ($0 <= 0).
	slt3 $0, $2, $1
	assert $0, 0

okay:	
	pass
wrong:
	fail
	
